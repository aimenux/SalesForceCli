{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "UseUtcTimestamp": true,
        "TimestampFormat": "[HH:mm:ss:fff] "
      }
    }
  },
  "Settings": {
    "ClientId": "!PUT-YOUR-CLIENT-ID!",
    "ClientSecret": "!PUT-YOUR-CLIENT-SECRET!",
    "UserName": "!PUT-YOUR-USERNAME-ID!",
    "Password": "!PUT-YOUR-PASSWORD-ID!",
    "Endpoint": "!PUT-YOUR-ENDPOINT-ID!"
  } 
}